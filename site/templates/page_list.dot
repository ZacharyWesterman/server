{{?it.count > 0}}
	{{? it.total}}
		Total {{= it.total}} result{{? it.total !== 1}}s{{?}}.<br>
	{{?}}

	{{?it.count > 9}}
		<button class="icon alt" *click="navigate_to_page(0)"><i class="fa-solid fa-caret-left fa-lg"></i></button>
		{{const start = Math.max(it.current - (environment.mobile ? 3 : 8), 0);const end = Math.min(start + (environment.mobile ? 7 : 17), it.count);}}
		{{~it.pages.slice(start, end) : page}}
			<button class="icon {{?page !== it.current}}alt{{?}}" *click="navigate_to_page({{=page}})">{{=page+1}}</button>
		{{~}}
		<button class="icon alt" *click="navigate_to_page({{=it.count-1}})"><i class="fa-solid fa-caret-right fa-lg"></i></button>
	{{??}}
		{{~it.pages : page}}
			<button class="icon {{?page !== it.current}}alt{{?}}" *click="navigate_to_page({{=page}})">{{=page+1}}</button>
		{{~}}
	{{?}}
{{?? it.no_results_msg}}
	<p>{{= it.no_results_msg}}</p>
{{?}}
