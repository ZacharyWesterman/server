{{? it.length }}
<hr>
<p>Edit existing feeds:</p>
<table>
	<tr>
		<th style="border-right: 1px solid var(--secondary-text)">Name</th>
		<th>Type</th>
		<th>
			<span class="tooltip" *click="help_types">
				<i class="fa-solid fa-circle-question"></i>
				<span class="tooltiptext t-bottom t-left">What are feed types?</span>
			</span>
		</th>
	</tr>

	{{~ it: feed }}
	<tr>
		<td style="border-right: 1px solid var(--secondary-text)">{{= feed.name }}</td>
		<td>{{= feed.kind.display || feed.kind.value }}</td>
		<td>
			<button class="icon clickable tooltip alt right" *click="delete_feed('{{= feed.id }}')">
				<i class="fa-solid fa-trash fa-lg disabled"></i>
				<span class="tooltiptext t-float-left">Delete this feed</span>
			</button>
		</td>
	</tr>
	{{~}}
</table>
{{?}}
